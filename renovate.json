{
  "packageRules": [
    {
    "matchPackageNames": ["nodemon", "chalk", "xmldoc"],
    "groupName": "multiple limits"
    },
    {
      "matchPackageNames": ["nodemon"],
      "prHourlyLimit": 5,
      "prConcurrentLimit": 5,
      "branchConcurrentLimit": 5
    },
    {
      "matchPackageNames": ["chalk"],
      "prHourlyLimit": 5,
      "prConcurrentLimit": 5,
      "branchConcurrentLimit": 5
    },
    {
      "matchPackageNames": ["xmldoc"],
      "prHourlyLimit": 1,
      "prConcurrentLimit": 1
    },
    {
      "matchPackageNames": ["react"],
      "prPriority": 10
    }
  ]
}
